<h1>Show Article</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @article.name %>
  </li>

  <li>
    <strong>Content:</strong>
    <%= @article.content %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.article_path(@conn, :edit, @article) %></span> |
<span><%= link "Back", to: Routes.article_path(@conn, :index) %></span>
